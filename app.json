{
  "expo": {
    "name": "Lorent",
    "slug": "Lorent",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Lorenta-1.png",
    "scheme": "Lorent",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.manojrustcult.rfid"
    },
    "android": {
      "permissions": [
        "BLUETOOTH",
        "BLUETOOTH_ADMIN",
        "BLUETOOTH_SCAN",
        "BLUETOOTH_CONNECT",
        "ACCESS_FINE_LOCATION"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/Lorenta-1.png",
        "backgroundImage": "./assets/images/android-icon-background.png"
      },
      "package": "com.manojrustcult.rfid"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Lorenta-1.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Lorenta-1.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#38208C",
          "dark": {
            "image": "./assets/images/Lorenta-1.png",
            "backgroundColor": "#38208C"
          }
        }
      ],
      "react-native-ble-plx",
      "expo-secure-store"
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}